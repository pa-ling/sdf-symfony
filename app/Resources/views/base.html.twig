<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <title>{% block title %}SDF{% endblock %}</title>

    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.png?v=2') }}"/>
    <link rel="shortcut icon" href="{{ asset('favicon.png?v=2') }}" type="img/x-icon">

    {% stylesheets '@bootstrap_css' %}
        <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}"/>
    {% endstylesheets %}

    <link rel="stylesheet" type="text/css" media="all" href="{{ asset('css/style.css') }}">
    <link rel="stylesheet" type="text/css" media="all" href="{{ asset('css/symfony.css') }}">

    <script href="{{ asset('js/simpleCart.min.js') }}"></script>

    <script href="{{ asset('js/jquery.easydropdown.js') }}"></script>

    <link rel="stylesheet" type="text/css" media="all" href="{{ asset('css/memenu.css') }}">
    <script href="{{ asset('js/memenu.js') }}"></script>

    {% javascripts '@jquery' '@bootstrap_js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    <style>
        body {
            padding-top: 70px;
        }    
    </style>

    {% block style %}
    {% endblock %}
</head>

<body>
{% block nav %}
    {%- include 'nav.html.twig' -%}
{% endblock %}

{% if app.user %} 
    {% block sidebar %}
    {% endblock %}
{% endif %}

{% block body %}

{% endblock %}



{% block footer %}
    {%- include 'footer.html.twig' -%}
{% endblock %}

</body>
</html>